    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/daftar": {
            "post": {
                "description": "Membuat akun baru nasabah",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Daftar Nasabah"
                ],
                "summary": "Membuat akun baru nasabah",
                "parameters": [
                    {
                        "description": "Masukkan nama, nik (16 Digit, Angka) dan no_hp (9-14 Digit, Angka)",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.InputPostDaftar"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ResponsePostDaftar"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/mutasi/{no_rekening}": {
            "get": {
                "description": "Melihat mutasi dari Rekening",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Melihat Mutasi"
                ],
                "summary": "Melihat mutasi dari Rekening",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Nomor Rekening",
                        "name": "no_rekening",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Mutasi"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/saldo/{no_rekening}": {
            "get": {
                "description": "Melihat saldo dari Rekening",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Melihat Saldo"
                ],
                "summary": "Melihat saldo dari Rekening",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Nomor Rekening",
                        "name": "no_rekening",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ResponseGetSaldoByNoRekening"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/tabung": {
            "post": {
                "description": "Menambah tabungan ke Rekening",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Menambah Tabungan"
                ],
                "summary": "Menambah tabungan ke Rekening",
                "parameters": [
                    {
                        "description": "Masukkan no_rekening, nominal (minimal 1)",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.InputPostTabung"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ResponsePostTabung"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/tarik": {
            "post": {
                "description": "Menarik saldo dari Rekening",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Menarik Saldo"
                ],
                "summary": "Menarik saldo dari Rekening",
                "parameters": [
                    {
                        "description": "Masukkan no_rekening, nominal tarik (minimal 1)",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.InputPostTarik"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ResponsePostTarik"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/dto.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.ErrorResponse": {
            "type": "object",
            "properties": {
                "remark": {
                    "type": "string"
                }
            }
        },
        "dto.InputPostDaftar": {
            "type": "object",
            "required": [
                "nama",
                "nik",
                "no_hp"
            ],
            "properties": {
                "nama": {
                    "type": "string"
                },
                "nik": {
                    "type": "string"
                },
                "no_hp": {
                    "type": "string",
                    "maxLength": 14,
                    "minLength": 10
                }
            }
        },
        "dto.InputPostTabung": {
            "type": "object",
            "required": [
                "no_rekening",
                "nominal"
            ],
            "properties": {
                "no_rekening": {
                    "type": "string"
                },
                "nominal": {
                    "type": "integer",
                    "minimum": 1
                }
            }
        },
        "dto.InputPostTarik": {
            "type": "object",
            "required": [
                "no_rekening",
                "nominal"
            ],
            "properties": {
                "no_rekening": {
                    "type": "string"
                },
                "nominal": {
                    "type": "integer",
                    "minimum": 1
                }
            }
        },
        "dto.ResponseGetSaldoByNoRekening": {
            "type": "object",
            "properties": {
                "saldo": {
                    "type": "integer"
                }
            }
        },
        "dto.ResponsePostDaftar": {
            "type": "object",
            "properties": {
                "no_rekening": {
                    "type": "string"
                }
            }
        },
        "dto.ResponsePostTabung": {
            "type": "object",
            "properties": {
                "saldo": {
                    "type": "integer"
                }
            }
        },
        "dto.ResponsePostTarik": {
            "type": "object",
            "properties": {
                "saldo": {
                    "type": "integer"
                }
            }
        },
        "models.Mutasi": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "deleted_at": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "nasabah_id": {
                    "type": "integer"
                },
                "no_rekening": {
                    "type": "string"
                },
                "nominal_tabung": {
                    "type": "integer"
                },
                "nominal_tarik": {
                    "type": "integer"
                },
                "saldo_akhir": {
                    "type": "integer"
                },
                "saldo_awal": {
                    "type": "integer"
                },
                "tipe_transaksi": {
                    "$ref": "#/definitions/models.TipeTransaksi"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        },
        "models.TipeTransaksi": {
            "type": "string",
            "enum": [
                "tabung",
                "tarik"
            ],
            "x-enum-varnames": [
                "Tabung",
                "Tarik"
            ]
        }
    }
